 <animation>
  <type>select</type>
  <object-name>NoseGearStrutDoor</object-name>
  <object-name>NoseGearStrut</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NoseGearTire</object-name>
  <object-name>NoseGearStrutLower</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>LMainGear</object-name>
  <object-name>LMainTire</object-name>
  <object-name>RMainGear</object-name>
  <object-name>RMainTire</object-name>
  <condition>
   <greater-than>
    <property>gear/gear[0]/position-norm</property>
    <value>0.15</value>
   </greater-than>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NoseGearDoor</object-name>
  <object-name>LMainGearDoorInner</object-name>
  <object-name>RMainGearDoorInner</object-name>
  <condition>
   <and>
   <not-equals>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </not-equals>
   <not-equals>
    <property>gear/gear[0]/position-norm</property>
    <value>1</value>
   </not-equals>
   </and>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NoseWellCoverAft</object-name>
  <object-name>MainWellCoversInner</object-name>
  <condition>
   <or>
    <equals>
     <property>gear/gear[0]/position-norm</property>
     <value>0</value>
    </equals>
    <equals>
     <property>gear/gear[0]/position-norm</property>
     <value>1</value>
    </equals>
   </or>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>NoseWellCoverFwd</object-name>
  <object-name>MainWellCoversOuter</object-name>
  <condition>
   <equals>
    <property>gear/gear[0]/position-norm</property>
    <value>0</value>
   </equals>
  </condition>
 </animation>

